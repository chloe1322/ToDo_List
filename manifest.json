{
    "manifest_version": 2,
    "name": "ToDo List",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["styles.css"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}